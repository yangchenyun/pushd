    "async": "~0.2.5",
    "express": "~3.1.0",
    "hiredis": "~0.1.14",
    "redis": "~0.8.2",
    "netmask": "~0.0.2",
    "request": "~2.14.0"
  },
  "devDependencies": {
    "mocha": "*",
    "should": "*"
  },
  "licenses": [
    {
      "type": "MIT",
